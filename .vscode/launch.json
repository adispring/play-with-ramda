  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/src/index.js",
      "outFiles": [ "${workspaceRoot}/.compiled/**/*.js" ],
      "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v12.0.0/bin/node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Current File",
      "program": "${file}",
      "outFiles": [ "${workspaceRoot}/.compiled/**/*.js" ]
    }
  ]
}